{
  "id": "plugin.com.AI-auto.deepseek",
  "plugin_type": "translate",
  "icon": "auto.svg",
  "display": "AI-auto",
  "homepage": "https://github.com/tzulao55/pot-app-translate-plugin-deepseek",
  "needs": [
    {
      "key": "requestPath",
      "display": "自定义API地址",
      "type": "input",
      "default": "https://api.deepseek.com/chat/completions",
      "placeholder": "例如: https://api.your-proxy.com/v1"
    }，
    {
      "key": "model",
      "display": "模型名称",
      "type": "input",
      "default": "deepseek-chat"
    }，
    {
      "key": "apiKey",
      "display": "API密钥 (多个请用,隔开)",
      "type": "input"
    }
  ]，
  "language": {
    "auto": "auto",
    "zh_cn": "zh",
    "zh_tw": "zh_HANT",
    "en": "en",
    "ja": "ja",
    "ko": "ko",
    "fr": "fr",
    "es": "es",
    "ru": "ru",
    "de": "de",
    "it": "it",
    "tr": "tr",
    "pt_pt": "pt",
    "pt_br": "pt",
    "vi": "vi",
    "id": "id",
    "th": "th",
    "ms": "ms",
    "ar": "ar",
    "hi": "hi",
    "mn_cy": "mn",
    "mn_mo": "mn",
    "km": "km",
    "nb_no": "no",
    "nn_no": "no",
    "fa": "fa"
  }
}
